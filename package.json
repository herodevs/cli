{
  "name": "@herodevs/cli",
  "version": "0.0.1",
  "description": "HeroDevs NES Developer Kit",
  "author": "@herodevs",
  "bin": {
    "@herodevs/cli": "./bin/run",
    "hd": "./bin/run"
  },
  "homepage": "https://github.com/herodevs/cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "herodevs/cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^3.2.0",
    "@oclif/plugin-update": "^3.1.32",
    "date-fns": "^2.30.0",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@oclif/test": "^2.4.4",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.40",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3.14.0",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.1",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "@herodevs/cli",
    "dirname": "@herodevs/cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-update"
    ],
    "topicSeparator": " ",
    "topics": {
      "committer": {
        "description": "Committer actions"
      }
    }
  },
  "scripts": {
    "generate:command": "oclif generate command",
    "start": "npm run dev",
    "predev": "npm run build",
    "dev": "./bin/dev",
    "preprod": "npm run build",
    "prod": "./bin/run",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/herodevs/cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
