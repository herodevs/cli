{
  "name": "@herodevs/cli",
  "description": "The HeroDevs CLI",
  "version": "2.0.0",
  "author": "HeroDevs, Inc",
  "bin": {
    "hd": "./bin/run.js"
  },
  "type": "commonjs",
  "scripts": {
    "dev": "npm run build && ./bin/dev.js \"$@\"",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.13.1",
    "@inquirer/prompts": "^5.0.2",
    "cli-spinners": "^3.0.0",
    "date-fns": "^3.6.0",
    "git-last-commit": "^1.0.1",
    "graphql": "^16.8.1",
    "isomorphic-fetch": "^3.0.0",
    "minimist": "^1.2.8",
    "ora": "5.3.0",
    "sloc": "^0.3.2",
    "tslib": "^2.3.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@nx/esbuild": "19.6.1",
    "@nx/eslint": "19.8.14",
    "@nx/eslint-plugin": "19.6.1",
    "@nx/jest": "19.6.1",
    "@nx/js": "19.6.1",
    "@nx/workspace": "19.8.14",
    "@swc-node/register": "1.9.2",
    "@swc/core": "1.5.7",
    "@swc/helpers": "0.5.12",
    "@types/jest": "29.5.12",
    "@types/minimist": "^1.2.5",
    "@types/node": "18.19.68",
    "@types/sloc": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "esbuild": "^0.19.12",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "29.7.0",
    "jest-environment-node": "^29.4.1",
    "nx": "19.6.1",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.8.2",
    "verdaccio": "^5.0.4"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/inquirer": "^9.0.7",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@types/node-fetch": "^2.6.12",
    "@types/sinon": "^17.0.4",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "mocha-clearscreen-reporter": "^1.0.1",
    "nock": "^14.0.1",
    "oclif": "^4",
    "shx": "^0.3.3",
    "sinon": "^19.0.2",
    "ts-node": "^10",
    "tsx": "^4.19.3",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/@herodevs/cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "hd",
    "dirname": "hd",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    },
    "hooks": {
      "prerun": "./dist/hooks/prerun/CommandContextHook"
    }
  },
  "repository": {
      "type": "git",
      "url": "https://github.com/herodevs/cli.git"
  },
  "types": "dist/index.d.ts"
}
